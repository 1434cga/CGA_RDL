stcI_HASH : Module_Name
stcI_FILEPREFIX : KEYManagerService/src/
stcI_FILEBODY : NO
stcI_FILEPOSTFIX : Makefile
stcI_EXTENSION : am
Set : $iterate_comments = OFF
SetI : $MODULENAME = KEY
# Makefile.am - make executable for service layer process
bin_PROGRAMS = +<+ucfirst($MODULENAME)+>+ManagerService

+<+ucfirst($MODULENAME)+>+ManagerService_SOURCES :=\
    $(top_srcdir)/+<+ucfirst($MODULENAME)+>+ManagerService_main.cpp \
    $(top_srcdir)/+<+ucfirst($MODULENAME)+>+InputManager.cpp \
    $(top_srcdir)/+<+ucfirst($MODULENAME)+>+ManagerService.cpp

+<+ucfirst($MODULENAME)+>+ManagerService_CPPFLAGS := \
    -I$(top_srcdir) \
    -I$(top_srcdir)/include \
    -include $(stincdir)/arch/linux-arm/OEConfig.h \
    -I$(workspace)/servicelayer/core/service_layer/include \
    -I$(stincdir)/servicelayer \
    -O2 \
    -g \
    -Wall \
    -Wno-unused-parameter \
    -fpermissive \
    -std=c++11 \
    $(AM_CPPFLAGS) \
    -fstack-protector-all \
    -D_FORTIFY_SOURCE=2 \
    -pie \
    -fPIE

+<+ucfirst($MODULENAME)+>+ManagerService_LDFLAGS = \
    -lserviceutils \
    -lwatchdog_client \
    -lcorebase \
    -lisystem \
    -liapplication \
    -litelephony \
    -lutils \
    -lbinder \
    -llog \
    -z,relro \
    -z,now \
    -pie \
    -liaudio \
    -lidiag \
    -lisystem \
    -lipower \
    -livif \
    -lilocation \
    -lproperty \
    -lihmi \
    -li+<+lc($MODULENAME)+>+