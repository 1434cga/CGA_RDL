stcI_HASH : Module_Name
stcI_EXTENSION : bb
stcI_FILEPREFIX : meta-file/meta-lge-model/recipes-mgrs/KEYManagerService/
stcI_FILEPOSTFIX : mgr-bin
Set : $iterate_comments = OFF
SetI : $MODULENAME = KEY
inherit autotools
inherit wave-features
inherit wave-debug-features
inherit wave-license

DESCRIPTION = "+<+ucfirst($MODULENAME)+>+ Manager service's bin"
SECTION = "+<+lc($MODULENAME)+>+mgr-bin"

PR = "r0"

FILESPATH := "${WAVE_SRC_DIR}/+<+ucfirst($MODULENAME)+>+ManagerService:"
SRC_URI = "file://bin/"

S = "${WORKDIR}/bin/"

DEPENDS = "servicelayer +<+lc($MODULENAME)+>+mgr-lib"
