stcI_HASH : Module_Name
stcI_EXTENSION : bb
stcI_FILEPREFIX : meta-file/meta-lge-model/recipes-mgrs/KEYManagerService/
stcI_FILEPOSTFIX : mgr-lib
Set : $iterate_comments = OFF
SetI : $MODULENAME = KEY
inherit autotools
inherit wave-features
inherit wave-debug-features
inherit wave-license

DESCRIPTION = "+<+ucfirst($MODULENAME)+>+ Manager service's lib"
SECTION = "+<+lc($MODULENAME)+>+mgr-lib"

PR = "r0"

FILESPATH := "${WAVE_SRC_DIR}/+<+ucfirst($MODULENAME)+>+ManagerService:"
SRC_URI = "file://lib/"
SRC_URI += "file://include/"

S = "${WORKDIR}/lib/"

CPPFLAGS += "-I${STAGING_INCDIR}/servicelayer"
DEPENDS = "servicelayer"
