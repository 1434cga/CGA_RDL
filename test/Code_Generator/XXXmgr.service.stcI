stcI_HASH : Module_Name
stcI_EXTENSION : service
stcI_FILEPREFIX : meta-file/meta-tiger-model/recipes-core/systemd-services/files/model-services/
stcI_FILEPOSTFIX : mgr
Set : $iterate_comments = OFF
SetI : $MODULENAME = KEY
[Unit]
Description=+<+ucfirst($MODULENAME)+>+ Manager Service
SourcePath=/usr/bin/+<+ucfirst($MODULENAME)+>+MgrService
After=start_servicemanager.service start_start.service applicationManager.service

[Service]
Type=notify
Restart=on-failure
#RestartSec=20s
WatchdogSec=60s
EnvironmentFile=/etc/systemd/TigrisEnvironment
ExecStart=/usr/bin/+<+ucfirst($MODULENAME)+>+MgrService
TimeoutSec=3s
#StartLimitInterval=480s
#StartLimitBurst=3
#StartLimitAction=reboot-force

[Install]
WantedBy=multi-user.target
